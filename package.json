{
  "name": "twiddet",
  "version": "0.0.0",
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "auth0-js": "^9.10.0",
    "babel-plugin-styled-components": "^1.10.0",
    "express": "^4.16.4",
    "js-cookie": "2.2.0",
    "jwt-decode": "2.2.0",
    "next": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "ts-node --project tsconfig.server.json server",
    "dev": "nodemon"
  },
  "devDependencies": {
    "@types/auth0-js": "^8.11.11",
    "@types/express": "^4.16.1",
    "@types/js-cookie": "^2.2.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/next": "^8.0.1",
    "@types/node": "^11.9.4",
    "@types/styled-components": "^4.1.10",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "**/*.test.ts",
      "/.git",
      "node_modules",
      ".next"
    ],
    "watch": [
      "server/**/*"
    ],
    "exec": "npm start",
    "ext": "js json ts"
  }
}
